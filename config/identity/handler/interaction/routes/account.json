{
  "@context": "https://linkedsoftwaredependencies.org/bundles/npm/@solid/community-server/^5.0.0/components/context.jsonld",
  "@graph": [
    {
      "comment": "Handles the account creation and redirection.",
      "@id": "urn:solid-server:auth:password:AccountRouteHandler",
      "@type": "InteractionRouteHandler",
      "route": {
        "@id": "urn:solid-server:auth:password:AccountRoute",
        "@type": "RelativePathInteractionRoute",
        "base": { "@id": "urn:solid-server:auth:password:IndexRoute" },
        "relativePath": "account/"
      },
      "source": {
        "@id": "urn:solid-server:auth:password:AccountHandler",
        "@type": "WaterfallHandler",
        "handlers": [
          {
            "@type": "MethodFilterHandler",
            "methods": [ "POST" ],
            "source": {
              "@id": "urn:solid-server:auth:password:AccountViewHandler",
              "@type": "CreateAccountHandler",
              "accountStore": { "@id": "urn:solid-server:auth:password:AccountStore" },
              "accountRoute": { "@id": "urn:solid-server:auth:password:AccountIdRoute" }
            }
          }
        ]
      }
    }
  ]
}
