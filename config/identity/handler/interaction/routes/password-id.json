{
  "@context": "https://linkedsoftwaredependencies.org/bundles/npm/@solid/community-server/^5.0.0/components/context.jsonld",
  "@graph": [
    {
      "comment": "Handles the password link details such as update and delete.",
      "@id": "urn:solid-server:auth:password:PasswordIdRouteHandler",
      "@type": "InteractionRouteHandler",
      "route": {
        "@id": "urn:solid-server:auth:password:PasswordIdRoute",
        "@type": "IdInteractionRoute",
        "base": { "@id": "urn:solid-server:auth:password:PasswordRoute" },
        "idName": "passwordId"
      },
      "source": {
        "@id": "urn:solid-server:auth:password:PasswordIdHandler",
        "@type": "WaterfallHandler",
        "handlers": [
          {
            "@type": "ViewInteractionHandler",
            "source": {
              "@type": "UpdatePasswordHandler",
              "accountStore": { "@id": "urn:solid-server:auth:password:AccountStore" },
              "passwordStore": { "@id": "urn:solid-server:auth:password:PasswordStore" },
              "passwordRoute": { "@id": "urn:solid-server:auth:password:PasswordIdRoute" }
            }
          },
          {
            "@type": "MethodFilterHandler",
            "methods": [ "DELETE" ],
            "source": {
              "@type": "DeletePasswordHandler",
              "accountStore": { "@id": "urn:solid-server:auth:password:AccountStore" },
              "passwordStore": { "@id": "urn:solid-server:auth:password:PasswordStore" },
              "passwordRoute": { "@id": "urn:solid-server:auth:password:PasswordIdRoute" }
            }
          }
        ]
      }
    }
  ]
}
